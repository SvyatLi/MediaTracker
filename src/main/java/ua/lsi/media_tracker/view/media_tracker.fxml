<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="537.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.lsi.media_tracker.controllers.MediaTrackerController">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<MenuBar prefHeight="25.0" prefWidth="551.0">
			<menus>
				<Menu mnemonicParsing="false" text="Data">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#loadData" text="Load">
							<accelerator>
								<KeyCodeCombination alt="UP" code="L" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
							</accelerator>
						</MenuItem>
						<MenuItem mnemonicParsing="false" onAction="#saveData" text="Save">
							<accelerator>
								<KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
							</accelerator>
						</MenuItem>
						<MenuItem mnemonicParsing="false" onAction="#saveDataWithDialog" text="Save as" />
						<MenuItem mnemonicParsing="false" onAction="#openAddItemDialog" text="Add Item" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Tools">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#openSettings" text="Settings" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Help">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#openAbout" text="About" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<ScrollPane id="scrollPane" prefHeight="365.0" prefWidth="580.0" GridPane.rowIndex="1">
			<content>
				<VBox id="scrollVBox" alignment="CENTER" prefHeight="200.0" prefWidth="580.0" />
			</content>
		</ScrollPane>
		<Label fx:id="statusLabel" maxWidth="2000.0" prefHeight="25.0" prefWidth="458.0" text="" textOverrun="CENTER_ELLIPSIS" wrapText="true" GridPane.rowIndex="2">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
	</children>
</GridPane>
